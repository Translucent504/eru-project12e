  "description": "A virtual lolly site with a serverless faunadb backend and custom build hooks",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "prebuild": "cd functions/lolly-graphql && npm i",
    "build": "gatsby build",
    "develop": "gatsby develop",
